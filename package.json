{
  "name": "qgis-js-demo",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "build": "slidev build --base /qgis-js-demo/",
    "dev": "slidev",
    "export": "slidev export",
    "preview": "vite preview --base /qgis-js-demo/",
    "update": "pnpm update"
  },
  "slidev": {
    "colorSchema": "light",
    "defaults": {
      "aspectRatio": "16/9",
      "fonts": {
        "sans": "Trueno",
        "mono": "Fira Code"
      }
    }
  },
  "dependencies": {
    "qgis-js": "0.0.5",
    "@qgis-js/ol": "0.0.5",
    "@qgis-js/utils": "0.0.5",
    "ol": "9.2.4",
    "@slidev/cli": "^0.49.29",
    "@slidev/client": "^0.49.29",
    "@slidev/theme-default": "^0.25.0",
    "coi-serviceworker": "^0.1.7"
  },
  "devDependencies": {
    "pnpm": "9.9.0",
    "vite": "^5.4.3",
    "prettier": "^3.3.3",
    "prettier-plugin-slidev": "^1.0.5"
  }
}